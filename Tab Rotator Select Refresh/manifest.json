{
  "manifest_version": 3,
  "name": "Tab Rotator select refresh",
  "version": "1.1",
  "description": "Automatically rotate through open tabs with selective refresh options",
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Tab Rotator select refresh"
  },
  "background": {
    "service_worker": "background.js"
  }
}